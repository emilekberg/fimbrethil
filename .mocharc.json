{
  "diff": true,
  "require": "ts-node/register",
  "extensions": [
    "ts"
  ],
  "reporter": "dot",
  "spec": "src/**/*.spec.ts",
  "file": [
    "src/index.ts"
  ],
  "package": "./package.json",
  "watch-files": ["src/**/*.ts"],
  "node-option": [
		"experimental-specifier-resolution=node",
		"loader=ts-node/esm"
	]
}