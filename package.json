{
  "name": "fimbrethil",
  "version": "1.1.2",
  "description": "entity component system javascript and typescript",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    "import": "./dist/es/index.js",
    "require": "./dist/cjs/index.js"
  },
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "test": "mocha --reporter spec",
    "coverage": "nyc --reporter lcov --reporter text-summary npm run test",
    "build": "tsc -p ./tsconfig.esm.json",
    "bundle": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emilekberg/fimbrethil.git"
  },
  "keywords": [
    "ecs",
    "entity-component-system"
  ],
  "author": "Emil Ekberg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emilekberg/fimbrethil/issues"
  },
  "homepage": "https://github.com/emilekberg/fimbrethil#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^9.1.0",
    "chai": "^4.2.0",
    "mocha": "^9.2.2",
    "nyc": "^15.0.1",
    "rollup": "^2.10.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.0.0",
    "typescript": "^4.6.3"
  }
}
